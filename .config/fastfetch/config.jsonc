{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "~/.config/fastfetch/kitty.txt",
    "type": "auto",
    "color": {
      "1": "bright_magenta"
    }
  },
  "modules": [
    "os",
    {
      "type": "cpuusage",
      "key": "CPU"
    },
    {
      "type": "memory",
      "key": "RAM"
    },
    {
      "type": "battery",
      "key": "Battery",
      "format": "{/4}{-}{/}{4}{?5} [{5}]{?}"
    }
  ]
}
